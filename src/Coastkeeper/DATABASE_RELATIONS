Volunteer
	has_many Patrol

Location
	has_many Section
	has_many Patrol
	belongs_to Datasheet

Section
	belongs_to Location
	has_many PatrolEntry

Datasheet
	has_many DatasheetCategory
	has_many Location

DatasheetCategory
	belongs_to Datasheet
	has_many DatasheetEntry

DatasheetEntry
	belongs_to DatasheetCategory
	has_many PatrolTally

Patrol
	belongs_to Volunteer
	belongs_to Location
	has_many PatrolEntry

PatrolEntry
	belongs_to Patrol
	belongs_to Section
	has_many PatrolTally

PatrolTally
	belongs_to PatrolEntry
	belongs_to DatasheetEntry
	